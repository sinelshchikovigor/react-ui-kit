{
  "name": "react-ui-kit",
  "version": "0.0.1",
  "description": "React UI Components Library",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/sinelshchikovigor/react-ui-kit.git",
    "type": "git"
  },
  "author": "Igor_Sinelshikov <sinelshikovigor@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "build": "cross-env NODE_ENV=production webpack",
    "test": "jest"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/ts-jest/preprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "globals": {
      "__TS_CONFIG__": {
        "module": "commonjs",
        "jsx": "react"
      }
    },
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "dependencies": {
    "@types/react": "0.14.49",
    "@types/react-dom": "0.14.18",
    "cross-env": "3.1.3",
    "react": "15.4.0",
    "react-dom": "15.4.0"
  },
  "devDependencies": {
    "@types/enzyme": "2.5.38",
    "@types/jest": "16.0.0",
    "@types/node": "6.0.48",
    "@types/react-addons-test-utils": "0.14.15",
    "awesome-typescript-loader": "2.2.4",
    "cross-env": "3.1.1",
    "css-loader": "0.23.1",
    "enzyme": "2.6.0",
    "file-loader": "0.9.0",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "jest": "17.0.3",
    "jest-cli": "17.0.3",
    "json-loader": "0.5.4",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "path": "0.12.7",
    "react-addons-test-utils": "15.4.0",
    "react-hot-loader": "1.3.0",
    "style-loader": "0.13.1",
    "ts-jest": "17.0.0",
    "tslint": "3.15.1",
    "typescript": "2",
    "typings": "1.4.0",
    "webpack": "1.13.3",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "0.14.1"
  }
}
